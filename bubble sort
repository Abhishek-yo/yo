#include<stdio.h>
int main(){
    int ar[]={5,6,3,9,2,0,2,7,2,2};

    for(int i=0;i<10;i++){
    
        for(int j=0;j<9;j++){
            if(ar[j]>ar[j+1]){
                int t=0;
                t=ar[j];
                ar[j]=ar[j+1];
                ar[j+1]=t;
            }
        }

    }
    for(int i= 0;i<10;i++){
        printf("%d",ar[i]);
    }
    return 0;
}
