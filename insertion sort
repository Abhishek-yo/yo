#include<stdio.h>
void insertionsort(int ar[],int len){
    
    for(int i=1;i<10;i++){
        int current = ar[i];
        int prev =i-1;
        while(prev >=0 && ar[prev]<current){
            ar[prev + 1] =ar[prev];
            prev--;

        }
        ar[prev +1]=current;
    }
}
int main(){
    int ar[]={5,6,3,9,2,0,2,7,2,2};
    int len=10;
    insertionsort(ar,len);
    for(int i= 0;i<10;i++){
        printf("%d",ar[i]);
    }
    return 0;

}
